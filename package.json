    "build:md": "node tools/MarkdownBuilder/index.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:common": "vue-cli-service test:unit ./tests/common/**/*.spec.ts"
